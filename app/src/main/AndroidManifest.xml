<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="newwest.mycity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".exploreActivity" />
        <activity android:name=".benchActivity" />
        <activity android:name=".RewardsActivity" />
        <activity android:name=".SelectionActivity" />
        <activity android:name=".FeedbackActivity" />
        <activity android:name=".exploreTreeActivity"></activity>
    </application>

</manifest>